%YAML 1.2
---
name: AnarchyTools LLDB Console
scope: lldb.console
hidden: true

contexts:
  main:
    - match: (STDOUT:)\s+(.*)
      captures:
        1: comment
        2: string
    - match: (STDERR:)\s+(.*)
      captures:
        1: comment
        2: keyword
    - match: (\(lldb\))\s+(.*)
      captures:
        1: support.class
        2: entity.name.function